[color]
	ui = true
[alias]
	st = status
	co = checkout
	ci = commit
	b = branch
        dc = diff --cached
        staged = diff --cached --ignore-all-spac
        l = log --oneline --decorate --graph
        d = diff
        a = add
        au = add -u
        pr = pull --rebase
[core]
	excludesfile = /Users/akiinyo/.gitignore_global
[push] default = tracking
